{
  "name": "fabian-lee-portfolio",
  "private": true,
  "description": "Fabian Lee's Portfolio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "build:clean": "rm -rf node_modules && rm -f package-lock.json && npm install"
  },
  "dependencies": {
    "circular-std": "^1.0.3",
    "gatsby": "^2.19.16",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-source-contentful": "^2.1.8",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-remark": "^2.6.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "env-cmd": "^9.0.3",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "post-merge": "npm install --no-package-lock",
      "post-rebase": "npm install",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabianlee1211/sugarfabby.com"
  }
}
