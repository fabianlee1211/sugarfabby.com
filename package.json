{
  "name": "fabian-lee-portfolio",
  "private": true,
  "description": "Fabian Lee's Portfolio",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "build:clean": "rm -rf node_modules && rm -f package-lock.json && npm install"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "autoprefixer": "^10.0.4",
    "framer-motion": "^2.9.5",
    "gatsby": "^2.24.80",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-mdx": "^1.2.43",
    "gatsby-plugin-netlify": "^2.3.13",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-postcss": "^3.3.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-remark-autolink-headers": "^2.10.0",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-images": "^3.10.0",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-source-contentful": "^2.3.37",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.20",
    "postcss": "^8.2.4",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "post-merge": "npm install --no-package-lock",
      "post-rebase": "npm install",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabianlee1211/sugarfabby.com"
  },
  "engines": {
    "node": "^12"
  }
}
